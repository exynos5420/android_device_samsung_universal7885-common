/boot            emmc   /dev/block/platform/13500000.dwmmc0/by-name/BOOT
/recovery        emmc   /dev/block/platform/13500000.dwmmc0/by-name/RECOVERY  flags=backup=1
/system          ext4   /dev/block/platform/13500000.dwmmc0/by-name/SYSTEM
/cache           ext4   /dev/block/platform/13500000.dwmmc0/by-name/CACHE
/data            ext4   /dev/block/platform/13500000.dwmmc0/by-name/USERDATA  flags=encryptable=footer;length=-16384
/efs             emmc   /dev/block/platform/13500000.dwmmc0/by-name/EFS       flags=display="EFS";backup=1
/misc            emmc   /dev/block/platform/13500000.dwmmc0/by-name/OTA

/external_sdcard vfat   /dev/block/mmcblk1p1 /dev/block/mmcblk1               flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg         vfat   /dev/block/sda1 /dev/block/sda                        flags=display="USB-OTG";storage;wipeingui;removable
